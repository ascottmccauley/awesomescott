{
  "name": "awesomescott",
  "version": "1.0.0",
  "description": "My finest Nuxt.js project",
  "author": "AwesomeScott",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "jest",
    "prettier:format-all": "prettier --write \"**/*.{vue,scss,sass,js}\"",
    "prettier:check": "prettier \"**/*.{vue,scss,sass,js}\" --list-different",
    "firebase:deploy:ci": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/pwa": "^2.6.0",
    "cross-env": "^5.2.0",
